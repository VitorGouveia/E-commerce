  "license": "MIT",
  "scripts": {
    "dev:database": "prisma migrate dev",
    "dev:server": "NODE_ENV=development ts-node-dev --respawn --transpile-only --ignore-watch node_modules,tests --no-notify --exit-child -r tsconfig-paths/register src/server.ts",
    "dev": "npm-run-all dev:*",
    "test:database": "prisma migrate reset",
    "test:server": "NODE_ENV=test jest --maxWorkers=50%",
    "test": "npm-run-all test:*",
    "build:folder": "rimraf ./build",
    "build:babel": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "build": "npm-run-all build:*",
    "start": "node build/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/sqlite3": "^3.1.7",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prisma": "2.21.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "2.21.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "pm2": "^4.5.6",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  }
}
